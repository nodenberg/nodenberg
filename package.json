{
  "name": "nodenberg",
  "version": "0.0.1",
  "description": "Excel report generator (Pure Express API Server)",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodenberg/nodenberg.git"
  },
  "keywords": [
    "excel",
    "report",
    "generator",
    "pdf",
    "print-settings",
    "placeholder",
    "api",
    "express"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec tsx src/server.ts",
    "build": "tsc",
    "start": "node scripts/init-libreoffice.js && node dist/server.js",
    "start:no-init": "node dist/server.js",
    "test": "node tests/test-api.js",
    "clean": "rm -rf dist",
    "init-libreoffice": "node scripts/init-libreoffice.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20.0.0",
    "nodemon": "^3.0.2",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  }
}
